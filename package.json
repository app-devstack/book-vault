{
  "name": "book-vault",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": ["drizzle-orm", "uuid", "@uidotdev/usehooks"]
      }
    }
  },
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "db:generate": "npx drizzle-kit generate",
    "db:generate:custom": "npx drizzle-kit generate --custom",
    "db:reset": "drizzle-kit drop",
    "deploy": "eas build --platform android --profile development",
    "preview": "eas build --platform android --profile preview",
    "preview:local": "eas build --platform android --profile preview --local",
    "preview:nocache": "bun run preview --clear-cache",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write --cache --cache-strategy metadata src/",
    "format:check": "prettier --check --cache --cache-strategy metadata src/ #CI/CDでのフォーマット検証用",
    "expo:check": "npx expo install --check",
    "knip": "bunx knip"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@tanstack/react-query": "^5.77.2",
    "@uidotdev/usehooks": "^2.4.1",
    "babel-plugin-inline-import": "^3.0.0",
    "drizzle-orm": "^0.43.1",
    "expo": "53.0.16",
    "expo-blur": "~14.1.4",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.6",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.3",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.2",
    "expo-linear-gradient": "~14.1.4",
    "expo-linking": "~7.1.6",
    "expo-media-library": "~17.1.7",
    "expo-router": "~5.1.2",
    "expo-splash-screen": "~0.30.8",
    "expo-sqlite": "~15.2.13",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.4",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-toast-message": "^2.3.0",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "uuid": "^11.1.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "typescript": "~5.8.3"
  },
  "private": true
}