{
  "name": "book-vault",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "drizzle-orm",
          "uuid",
          "@uidotdev/usehooks"
        ]
      }
    }
  },
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "db:generate": "npx drizzle-kit generate",
    "db:generate:custom": "npx drizzle-kit generate --custom",
    "db:reset": "drizzle-kit drop",
    "prebuild": "expo prebuild --platform android",
    "deploy": "eas build --platform android --profile development",
    "preview": "eas build --platform android --profile preview",
    "preview:local": "eas build --platform android --profile preview --local",
    "preview:nocache": "bun run preview --clear-cache",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write --cache --cache-strategy metadata src/",
    "format:check": "prettier --check --cache --cache-strategy metadata src/ #CI/CDでのフォーマット検証用",
    "expo:check": "npx expo install --check",
    "knip": "bunx knip",
    "postinstall": "patch-package",
    "preview:deploy": "bash src/script/preview-local-build.sh"
  },
  "dependencies": {
    "@expo/config-plugins": "~10.1.1",
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^5.2.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.0.14",
    "@tamagui/babel-plugin": "^1.132.15",
    "@tamagui/button": "^1.132.15",
    "@tamagui/config": "^1.132.15",
    "@tamagui/core": "^1.132.15",
    "@tamagui/lucide-icons": "^1.132.15",
    "@tanstack/react-query": "^5.77.2",
    "@uidotdev/usehooks": "^2.4.1",
    "babel-plugin-inline-import": "^3.0.0",
    "drizzle-orm": "^0.43.1",
    "drizzle-zod": "^0.8.2",
    "expo": "^53.0.0",
    "expo-blur": "~14.1.5",
    "expo-clipboard": "~7.1.5",
    "expo-constants": "~17.1.7",
    "expo-crypto": "~14.1.5",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-media-library": "~17.1.7",
    "expo-router": "~5.1.4",
    "expo-share-intent": "^3.2.3",
    "expo-splash-screen": "~0.30.10",
    "expo-sqlite": "~15.2.14",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "patch-package": "^8.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.61.1",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-toast-message": "^2.3.0",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5",
    "tamagui": "^1.132.15",
    "uuid": "^11.1.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
