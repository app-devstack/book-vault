{
  "expo": {
    "name": "book-vault",
    "slug": "book-vault",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/chi-book.png",
    "scheme": "bookvault",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": false

    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/chi-book.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.r.t7maru.bookvault",
      "newArchEnabled": false,
      "intentFilters": [
        {
          "action": "SEND",
          "category": ["DEFAULT"],
          "data": {
              "mimeType": "text/plain"
            }
        },
        {
          "action": "VIEW",
          "category": ["DEFAULT", "BROWSABLE"],
          "data": {
            "scheme": "https"
          }
        },
        {
          "action": "VIEW",
          "category": ["DEFAULT", "BROWSABLE"],
          "data": {
            "scheme": "http"
          }
        },
        {
          "action": "VIEW",
          "category": ["DEFAULT", "BROWSABLE"],
          "data": {
            "scheme": "bookvault"
          }
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/chi-book.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fc7bed07-d891-4182-8586-bee205259353"
      }
    },
    "owner": "r.t7maru"
  }
}
